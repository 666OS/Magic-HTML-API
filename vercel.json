{
    "version": 2,
    "builds": [
        {
            "src": "api/extract.py",
            "use": "@vercel/python@3.1.0",
            "config": {
                "maxLambdaSize": "15mb",
                "pythonVersion": "3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/extract.py"
        }
    ]
} 